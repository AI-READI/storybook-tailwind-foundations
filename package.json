{
  "name": "storybook-tailwind-foundations",
  "description": "Storybook tailwind foundations from tailwind.config.js as stories",
  "version": "1.0.1",
  "scripts": {
    "dev": "npm run storybook",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs/.vitepress/dist/example",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "release": "release-it"
  },
  "main": "./src/bin/index.js",
  "files": [
    "src"
  ],
  "exports": {
    ".": "./src/bin/index.js",
    "./initialize.js": "./src/bin/initialize.js",
    "./theme.json": "./theme.json"
  },
  "peerDependencies": {
    "@storybook/builder-vite": "^0.2.2",
    "vite": "^3.0.4",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@release-it/conventional-changelog": "^5.1.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.10",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "eslint": "^8.21.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-vue": "^9.3.0",
    "find-cache-dir": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "commondir": "^1.0.1",
    "make-dir": "^3.0.2",
    "pkg-dir": "^4.1.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "release-it": "^15.4.0",
    "storybook-tailwind-foundations": "file:storybook-tailwind-foundations-0.2.1.tgz",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.4",
    "vite": "^3.0.4",
    "vitepress": "^1.0.0-alpha.12",
    "vue": "^3.2.37",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.39.5"
  },
  "keywords": [
    "storybook-addons",
    "style",
    "tailwind",
    "css",
    "appearance",
    "layout",
    "storybook"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/renatomoor/storybook-tailwind-foundations"
  },
  "license": "MIT",
  "storybook": {
    "displayName": "Tailwind Foundations",
    "supportedFrameworks": [
      "vue"
    ],
    "icon": "https://pbs.twimg.com/profile_images/1468993891584073729/a_op8KnL_400x400.jpg"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Renato Moor",
    "email": "renatomoor1@gmail.com"
  }
}
